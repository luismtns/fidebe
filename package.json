{
  "name": "@luisbovo/fidebe-widget",
  "version": "0.1.1",
  "description": "Floating feedback widget for React applications with screenshot capture.",
  "license": "MIT",
  "main": "./dist/fidebe-widget.umd.js",
  "module": "./dist/fidebe-widget.es.js",
  "types": "./dist/FidebeWidget.d.ts",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint src --ext ts,tsx",
    "lint:fix": "yarn lint --fix",
    "type-check": "tsc --project tsconfig.json --pretty --noEmit",
    "ci": "yarn lint && yarn type-check",
    "prepare": "husky install"
  },
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/fidebe-widget.es.js",
      "require": "./dist/fidebe-widget.umd.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/PlazaCC/fidebe/"
  },
  "keywords": [
    "feedback",
    "widget",
    "screenshot"
  ],
  "author": "Luis Bovo (contato@luisbovo.com.br)",
  "bugs": {
    "url": "https://github.com/PlazaCC/fidebe/issues"
  },
  "homepage": "https://github.com/PlazaCC/fidebe#readme",
  "peerDependencies": {
    "react": ">=17.x"
  },
  "devDependencies": {
    "@types/node": "17.0.21",
    "@types/react": "17.0.40",
    "@types/react-dom": "17.0.13",
    "@vitejs/plugin-react": "1.2.0",
    "autoprefixer": "^10.4.14",
    "babel-eslint": "10.1.0",
    "eslint": "8.11.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-config-react-app": "7.0.0",
    "eslint-config-standard": "16.0.3",
    "eslint-config-standard-jsx": "10.0.0",
    "eslint-config-standard-react": "11.0.1",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "^5.5.3",
    "eslint-plugin-promise": "6.0.0",
    "eslint-plugin-react": "7.29.4",
    "husky": "7.0.4",
    "postcss": "^8.4.21",
    "prettier": "^3.6.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "tailwindcss": "^3.4.1",
    "typescript": "4.6.2",
    "vite": "2.9.17",
    "vite-plugin-dts": "0.9.10",
    "vite-plugin-linter": "0.2.4",
    "vite-tsconfig-paths": "3.4.1"
  }
}
